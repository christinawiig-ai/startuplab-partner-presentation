{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/web-presentation/index.html"
    },
    {
      "source": "/web-presentation/(.*)",
      "destination": "/web-presentation/$1"
    },
    {
      "source": "/brand-profile/(.*)",
      "destination": "/brand-profile/$1"
    },
    {
      "source": "/((?!web-presentation|brand-profile|design-elements).*)",
      "destination": "/web-presentation/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/web-presentation/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/brand-profile/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}